{
  "name": "WTraining",
  "version": "0.1.0",
  "description": "WTraining",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "console": "node console.js",
    "cover": "NODE_ENV=testing jest --coverage --runInBand --forceExit test/app",
    "test": "NODE_ENV=testing npm run migrations-test && NODE_ENV=testing jest test/app --runInBand --forceExit",
    "test-inspect": "NODE_ENV=testing node --inspect --debug-brk jest test/app.spec.js",
    "eslint-check": "eslint --print-config .eslintrc.js --ignore-pattern ./.eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint \"**/*.js\" --ignore-pattern ./.eslintrc.js",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint-fix": "eslint \"**/*.js\" --fix --ignore-pattern ./.eslintrc.js",
    "precommit": "npm run lint-diff",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "prestart": "npm run lint",
    "migrations": "sequelize db:migrate",
    "start": "nodemon --inspect server.js",
    "migrations-test": "NODE_ENV=testing ./node_modules/.bin/sequelize db:drop && NODE_ENV=testing ./node_modules/.bin/sequelize db:create && NODE_ENV=testing ./node_modules/.bin/sequelize db:migrate"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "author": "Wolox",
  "homepage": "https://github.com/manutuero/gb-express-js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manutuero/gb-express-js.git"
  },
  "bugs": {
    "url": "https://github.com/manutuero/gb-express-js/issues",
    "email": "tls@wolox.com.ar"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/console.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/migrations/**",
      "!**/config/**",
      "!**/scripts/**",
      "!**/docs/**"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-validator": "^6.2.0",
    "express-wolox-logger": "0.0.2",
    "install": "^0.13.0",
    "jwt-simple": "^0.5.1",
    "node-fetch": "^2.6.0",
    "npm": "^6.11.3",
    "pg": "^7.4.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^4.34.0",
    "umzug": "^2.1.0"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "6.24.1",
    "dictum.js": "^1.0.0",
    "dotenv": "^5.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.3.1",
    "eslint-config-wolox": "^2.2.1",
    "eslint-config-wolox-node": "^1.0.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.3",
    "jest": "^24.9.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prompt": "^1.0.0",
    "sequelize-cli": "^4.0.0",
    "supertest": "^3.4.2"
  }
}
